end

function uplayer()
 -- movement
 if btn(⬅️) then
  plr.px-=1
  plr.flp=false
  plr.moving = true
 end
 if btn(➡️) then
  plr.px+=1
  plr.flp=true
  plr.moving = true
 end
 if plr.moving 
 and not btn(⬅️)
 and not btn(➡️)
 then
  plr.moving = false
 end
 
end

function dplayer() 
 spr(plr.sp,plr.px,plr.py,1,1,plr.flp)
 if plr.moving then
  if time()-plr.anim_time>.1 then
   plr.anim_time=time()
   plr.sp+=1
   if plr.sp > 8 then
    plr.sp=3
   end
  end
	else
	 if time() - plr.anim_time>.3 then
		 plr.sp+=1
		 plr.anim_time=time()
		 if plr.sp>2 then
			 plr.sp=1
			end
		end
	end
end
